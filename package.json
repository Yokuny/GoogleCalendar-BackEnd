{
  "name": "google-calendar-integration",
  "version": "1.0.0",
  "description": "Sistema de integração entre calendário de eventos local e Google Calendar",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "start": "npm run build; node dist/server.js",
    "dev": "tsx watch --env-file .env ./src/server.ts",
    "test": "node --env-file .env.test node_modules/.bin/jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "node --env-file .env.test node_modules/.bin/jest --passWithNoTests --runInBand",
    "test:ci": "npm test -- --coverage",
    "test:coverage:html": "node --env-file .env.test node_modules/.bin/jest --coverage --coverageReporters=html"
  },
  "author": "Yokuny",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.8.3",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "googleapis": "^133.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.3.0",
    "mongoose": "^8.0.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.6",
    "@types/cors": "^2.8.14",
    "@types/express": "^4.17.19",
    "@types/jest": "^29.5.10",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/node": "^20.8.6",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^9.1.1",
    "randexp": "^0.5.3",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3",
    "typescript-transform-paths": "^3.4.6",
    "uuid": "^11.1.0"
  }
}
